name:                wybe
version:             0.1.0.0
synopsis:            Wybe compilation to LPVM and ultimately LLVM
cabal-version:       >= 1.22.2.0
Category:            Compilers
build-type:          Simple


executable wybemk
  default-language:    Haskell2010
  main-is:             wybemk.hs
  hs-source-dirs: .
  build-depends: array >= 0.5.1.0
               , base >=4.6 && <5
               , binary >= 0.7.5.0
               , bytedump
               , bytestring
               , containers
               , directory >= 1.2.5.0
               , filepath >= 1.4.0.0
               , haskeline
               , hex
               , llvm-general == 3.5.1.2
               , llvm-general-pure
               , mtl
               , old-time
               , parsec
               , process == 1.4.2.0
               , transformers >=0.3.0.0 && <0.5
               , cryptonite
               , tuple >= 0.3.0.2
               , ansi-terminal ==0.6.2.3
