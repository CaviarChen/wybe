[ProcDecl Public (ProcProto "yorn" [ProcParam "prompt" (Type "string" []) ParamIn,ProcParam "result" (Type "bool" []) ParamOut]) [Loop [NormalStmt (ProcCall "print" [Var "prompt"]),NormalStmt (ProcCall "print" [StringValue " (y/n) "]),NormalStmt (ProcCall "read_line" [Var "response"]),NormalStmt (Assign "responsechar" (Fncall "to_upper" [Fncall "first" [Var "response"]])),NormalStmt (Assign "result" (Equal (Var "responsechar") (CharValue 'Y'))),BreakIf (Or (Var "result") (Equal (Var "responsechar") (CharValue 'N'))),NormalStmt (ProcCall "println" [StringValue "Please answer 'yes' or 'no'."])]]]
