# use position
public type position = public position(x:int, y:int) end

public type posrec = public posrec(p:position, a:int) end

public proc foo(r1:posrec) use !io
    ?pos1 = r1.p
    x(!pos1, 1111) # Non-destructive
    !println(pos1.x)
end


?pos = position(100,100)
?rec = posrec(pos, 1)

!foo(rec)