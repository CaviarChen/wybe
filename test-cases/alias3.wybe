# shouldn't alias to the data structure if all
# properties of a data structure are mutated in the proc

use position

public proc replicate1(p1:position, ?p2:position) use !io
  ?p2 = p1
  !print("p1 expected (1, 1):")
  !printPosition(p1)
  x(!p2, 2) # Nondestructive
  y(!p2, 2) # Destructive
  !print("p1 expected (1, 1):")
  !printPosition(p1)
  !print("p2 expected (2, 2):")
  !printPosition(p2)
end

public proc bar use !io
  ?p1 = position(1,1)
  !replicate1(p1, ?p2)

  x(!p1, 555)
  !print("p1 expected (555, 1):")
  !printPosition(p1)
  !print("p2 expected (2, 2):")
  !printPosition(p2)
end

!bar