pub constructors(?T) [] | [|](head: ?T, tail:generic_list(?T))

pub def ++(x:generic_list(?T), y:generic_list(?T)):generic_list(?T) =
    if x = [ ?h | ?t]
    then [h|t++y]
    else y

pub def length(x:generic_list(?T)) : int = length1(x, 0)

def length1(x:generic_list(?T), acc:int) : int =
    if x = [ ?h | ?t] then length1(t, acc+1) else acc


