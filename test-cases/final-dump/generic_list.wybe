parameter @T
pub constructors [] | [|](head: @T, tail:generic_list(@T))

pub def ++(x:generic_list(@T), y:generic_list(@T)):generic_list(@T) =
    if x = [ ?h | ?t]
    then [h|t++y]
    else y

pub def length(x:generic_list(@T)) : int =
    if x = [ ?h | ?t] then 1 + length(t) else 0


