!read(?b:int)
!print("a = ") !println(a)
!print("b = ") !println(b)
!print("a + b = ") !println(a+b)
!print("a - b = ") !println(a-b)
if { a > b ::
    !println("a > b")
| otherwise ::
    !println("a <= b")
}


?c = -10
?d = 5
!print("c = ") !println(c)
!print("d = ") !println(d)
!print_comparison(c, d)


def {noinline} print_comparison(c:int, d:int) use !io {
    if { c > d ::
        !println("c > d")
    | otherwise ::
        !println("c <= d")
    }
}
