use position

public proc replicate(?p2:position) use !io
  ?p1 = position(101,102)
  ?p2 = position(p1.x, p1.y)
  x(!p1, 10000) # destructive update
  y(!p2, p1.y+1) # destructive update
  !print("expect p1(10000,102):")
  !printPosition(p1)
  !print("expect p2(101,103):")
  !printPosition(p2)
end

public proc foo use !io
  !replicate(?p2)
  x(!p2, 20000)
  !print("--- after x(!p2, 20000) - expect p2(20000,103):")
  !printPosition(p2)
end

!foo
