public type course  { public course(code:int, name:string) }
public type student { public student(id:int, major:course) }

public def printStudent(stu:student) use !io {
  !print("student id: ")
  !println(stu.id)
  ?cour = stu.major
  !print("course code: ")
  !println(cour.code)
  !print("course name: ")
  !println(cour.name)
}

!printStudent(student(12345,course(90048, "Declarative Programming")))
